<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.ConfigureSensorActivity">

    <include
        android:id="@+id/toolbar_support_configure"
        layout="@layout/toolbar" />

    <Button
        android:id="@+id/button_on_off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ON/OFF SENSOR"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_support_configure" />

    <Button
        android:id="@+id/button_health_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HEALTH CHECK"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_on_off" />
</androidx.constraintlayout.widget.ConstraintLayout>